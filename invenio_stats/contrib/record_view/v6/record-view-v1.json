{
  "index_patterns": ["events-stats-record-view-*"],
  "settings": {
    "index": {
      "refresh_interval": "5s"
    }
  },
  "mappings": {
    "stats-record-view": {
      "date_detection": false,
      "dynamic": false,
      "numeric_detection": false,
      "properties": {
        "timestamp": {
          "type": "date",
          "format": "strict_date_hour_minute_second"
        },
        "record_id": {
          "type": "keyword"
        },
        "pid_type": {
          "type": "keyword"
        },
        "pid_value": {
          "type": "keyword"
        },
        "labels": {
          "type": "keyword"
        },
        "country": {
          "type": "keyword"
        },
        "visitor_id": {
          "type": "keyword"
        },
        "unique_session_id": {
          "type": "keyword"
        },
        "referrer": {
          "type": "keyword"
        },
        "is_robot": {
          "type": "boolean"
        }
      }
    }
  },
  "aliases": {
    "events-stats-record-view": {}
  }
}
